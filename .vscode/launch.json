{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Azurite Mocha Tests",
            "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
            "args": [
                "--timeout",
                "999999",
                "--colors",
                "${workspaceFolder}/test/services/blob/*.js",
                "${workspaceFolder}/test/*.js"
            ],
            "env": {
                "NODE_ENV": "development",
                "AZURE_STORAGE_CONNECTION_STRING": "UseDevelopmentStorage=true;",
                "NOCK_OFF": "true"
            }
        },
        {
            "name": "Current Mocha",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
            "stopOnEntry": false,
            "args": ["--no-timeouts", "${workspaceFolder}/${relativeFile}"],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development",
                "AZURE_STORAGE_CONNECTION_STRING": "UseDevelopmentStorage=true;",
                "NOCK_OFF": "true"
            }
        },
        {
            "name": "Debug Node.js",
            "type": "node",
            "request": "launch",
            "stopOnEntry": false,
            "args": ["E:\\GitHub\\XiaoningLiu\\azure-storage-node\\examples\\samples\\retrypolicysample.js"],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": "node",
        },
    ]
}